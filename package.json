{
  "name": "@resin/abstract-sql-compiler",
  "version": "6.2.0",
  "description": "A translator for abstract sql into sql.",
  "main": "out/AbstractSQLCompiler.js",
  "types": "out/AbstractSQLCompiler.d.ts",
  "scripts": {
    "pretest": "npm run lint && npm run prepare",
    "test": "mocha",
    "prepublish": "require-npm4-to-publish",
    "prepare": "tsc",
    "lint:coffee": "resin-lint test/",
    "lint:typescript": "resin-lint --typescript src/ typings/ && tsc --noEmit",
    "lint": "npm run lint:coffee && npm run lint:typescript",
    "prettify": "prettier --config ./node_modules/resin-lint/config/.prettierrc --write \"{src,typings}/*.ts\"",
    "precommit": "lint-staged"
  },
  "repository": "https://github.com/resin-io-modules/abstract-sql-compiler.git",
  "author": "",
  "dependencies": {
    "@resin/sbvr-types": "^2.0.0",
    "@types/bluebird": "^3.5.24",
    "@types/lodash": "^4.14.118",
    "@types/node": "^8.10.37",
    "bluebird": "^3.5.3",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@resin/lf-to-abstract-sql": "2.1.0",
    "@resin/odata-parser": "^1.0.0",
    "@resin/odata-to-abstract-sql": "2.4.0",
    "@resin/sbvr-parser": "^0.2.1",
    "chai": "^4.2.0",
    "coffeescript": "^1.12.7",
    "husky": "^1.1.4",
    "lint-staged": "^8.0.4",
    "mocha": "^5.2.0",
    "prettier": "^1.15.2",
    "require-npm4-to-publish": "^1.0.0",
    "resin-lint": "^2.0.1",
    "typescript": "^3.1.6"
  }
}
