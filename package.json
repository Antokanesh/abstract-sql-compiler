{
  "name": "@resin/abstract-sql-compiler",
  "version": "6.5.0",
  "description": "A translator for abstract sql into sql.",
  "main": "out/AbstractSQLCompiler.js",
  "types": "out/AbstractSQLCompiler.d.ts",
  "scripts": {
    "pretest": "npm run lint && npm run prepare",
    "test": "mocha",
    "prepublish": "require-npm4-to-publish",
    "prepare": "tsc --project ./tsconfig.build.json",
    "lint:coffee": "resin-lint test/",
    "lint:typescript": "resin-lint --typescript src/ typings/ test/ && tsc --noEmit",
    "lint": "npm run lint:coffee && npm run lint:typescript",
    "prettify": "prettier --config ./node_modules/resin-lint/config/.prettierrc --write \"{src,typings,test}/**/*.ts\""
  },
  "repository": "https://github.com/resin-io-modules/abstract-sql-compiler.git",
  "author": "",
  "dependencies": {
    "@resin/sbvr-types": "^2.0.3",
    "@types/bluebird": "^3.5.27",
    "@types/lodash": "^4.14.132",
    "@types/node": "^8.10.48",
    "bluebird": "^3.5.5",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@resin/lf-to-abstract-sql": "2.1.0",
    "@resin/odata-parser": "1.0.3",
    "@resin/odata-to-abstract-sql": "3.0.1",
    "@resin/sbvr-parser": "0.2.2",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "chai": "^4.2.0",
    "coffeescript": "^1.12.7",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "mocha": "^6.1.4",
    "prettier": "^1.17.1",
    "require-npm4-to-publish": "^1.0.0",
    "resin-lint": "^2.0.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.5.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
